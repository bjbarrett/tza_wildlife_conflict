@misc{Mfunda2011,
abstract = {Crop-raiding by wildlife is a common concern in areas where agriculture plays an important role for sustaining rural livelihoods. Different techniques have been used to prevent crop loss by wildlife. This study examined wildlife crop damage as reported and experienced by people in villages surrounding Serengeti National Park (SNP). The results showed that generally crop production was not the only important economic activity people relied on. We conclude that hunting could be an alternative to crop production. Furthermore, education, employment status, wealth, immigration status and location influenced crop production significantly. Perceptions towards problematic wildlife varied between districts, but small- to medium-sized wildlife were perceived most problematic to crop production. Furthermore, respondents identified climate change factors and inadequate agriculture extension services to affect crop production. The study proposes further development of income-generating activities such as beekeeping and ecotourism as alternatives to crop production. These natural resources-based activities address food insecurity and increase livelihood options. Compensation for extreme cases of crop destruction and improved capacity and delivery of extension services could make agriculture around Serengeti more sustainable. {\textcopyright} 2011 Taylor {\&} Francis.},
author = {Mfunda, Iddi M. and R{\o}skaft, Eivin},
booktitle = {International Journal of Biodiversity Science, Ecosystem Services and Management},
doi = {10.1080/21513732.2011.602028},
file = {:C$\backslash$:/Users/Kate/Dropbox/Diss{\_}Prop/HWC{\_}risk/Wildlife or crop production the dilemma of conservation and human livelihoods in Serengeti Tanzania.pdf:pdf},
issn = {21513740},
keywords = {Tanzania,agriculture,crop-raiding,local people,natural resources,poverty,sustainable use,wildlife},
number = {1},
pages = {39--49},
title = {{Wildlife or crop production: The dilemma of conservation and human livelihoods in Serengeti, Tanzania}},
volume = {7},
year = {2011}
}
@article{Pekor2019,
abstract = {The fencing of protected areas (PAs) is highly controversial, and much remains unknown about the associated financial, ecological, and social impacts. We surveyed experts on 63 fenced and 121 unfenced PAs across 23 African countries to assess the advantages and drawbacks of fencing. Where fences exist, they are largely supported and widely viewed as effective at demarcating PA boundaries and mitigating human-wildlife conflicts. However, most fences were insufficiently funded, which limited their ability to contain conflict-prone species like elephants and lions. Fences were also frequently vandalised and caused numerous conflicts with local communities. We documented for the first time the distribution of and support for fencing in PAs across Africa. While fencing is largely limited to Southern Africa and East Africa, support for fencing is greatest in West Africa and is associated with high human and livestock densities, and high threats from bushmeat harvesting, livestock encroachment, and logging.},
author = {Pekor, Adam and Miller, Jennifer R.B. and Flyman, Michael V. and Kasiki, Samuel and Kesch, M. Kristina and Miller, Susan M. and Uiseb, Kenneth and van der Merve, Vincent and Lindsey, Peter A.},
doi = {10.1016/j.biocon.2018.10.030},
file = {:C$\backslash$:/Users/Kate/Dropbox/Diss{\_}Prop/HWC{\_}risk/Pekor2019.pdf:pdf},
issn = {00063207},
journal = {Biological Conservation},
number = {November 2018},
pages = {67--75},
title = {{Fencing Africa's protected areas: Costs, benefits, and management issues}},
volume = {229},
year = {2019}
}
@article{Nelson2003,
abstract = {This review focuses on current human-elephant conflict mitigation strategies by grouping those with similar components, and attempting to assess each strategy group in terms of their successes and failures. The authors note two themes emerged from the review: 'firstly, that no single method works in isolation, but rather that combinations provide the best chance of success; and secondly, that it is of fundamental importance to include those who are most affected by the problem in the solution'. Conflict alleviation is a two sided equation in that both elephant and people are in conflict, and 'the goal is to enable coexistance and sharing of resources on some scale'. Both sides of the equation must be addressed. Effective land use planning and tolerance for elephants can bring about some alleviation, but new stategies need to be developed and constantly revised in addressing the problems associated with crop-raiding elephants. The authors conclude that the key concept of effective alluviation of human-elephant conflict is 'adaptive management'.},
author = {Nelson, Alastair and Bidwell, Posy and Sillero-zubiri, Claudio},
