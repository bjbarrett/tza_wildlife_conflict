  file      = {:C$\backslash$:/Users/Kate/Dropbox/Diss{\_}Prop/Carter/Methodological evaluation of vegetation indexes in land use and land cover LULC classification.pdf:pdf},
  keywords  = {Landscape,Remote sensing,image,image processing,landscape,processing,remote sensing},
